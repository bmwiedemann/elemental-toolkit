set kernel=/boot/vmlinuz
set kernelcmd="console=tty1 console=ttyS0 cos-img/filename=$img panic=5 security=selinux selinux=1 rd.neednet=1 rd.cos.oemlabel=@OEM_LABEL@ fsck.mode=force fsck.repair=yes"
set recoverykernelcmd="console=tty1 console=ttyS0 cos-img/filename=$img panic=5 rd.neednet=1 rd.cos.oemlabel=@OEM_LABEL@"
set initramfs=/boot/initrd
